// Bit operators

func testNat8(a : Nat8, b : Nat8) {
   let not1 = ^ a;
   let not2 = (^ a) : Nat8;
   let or1 = a | b;
   let or2 = (a | b) : Nat8;
   let and1 = a & b;
   let and2 = (a & b) : Nat8;
   let xor = a ^ b;
   let xor2 = (a ^ b) : Nat8;
   let shl1 = a << b;
   let shl2 = a << b : Nat8;
   let shr1 = a >> b;
   let shr2 = a >> b : Nat8;
   let rotl1 = a <<> b;
   let rotl2 = a <<> b : Nat8;
   let rotr1 = a <>> b;
   let rotr2 = a <>> b : Nat8;
};


